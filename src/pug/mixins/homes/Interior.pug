mixin interiorHeader
  section.p-0.position-relative#interiorHeader
    .position-absolute.all-0(data-zanim-lg!={
      delay: 0.4,
      animation: "zoom-out"
    } data-zanim-trigger="scroll")
      .swiper.theme-slider(data-swiper!={
        loop: true,
        effect: 'fade',
      } data-zanim-lg!={animation: "zoom-out", delay: 0}  data-zanim-trigger='scroll')
        .swiper-wrapper
          .swiper-slide.min-vh-100.w-100
            +Background({image: `${CWD}assets/img/interior-1.jpg`})
          .swiper-slide.min-vh-100.w-100  
            +Background({image: `${CWD}assets/img/interior-4.jpg`})
          .swiper-slide.min-vh-100.w-100  
            +Background({image: `${CWD}assets/img/interior-6.jpg`})
        .swiper-nav
          .swiper-button-next.me-md-3
          .swiper-button-prev.ms-md-3 
    

    .container
      .row.vh-100.flex-center.justify-content-lg-start.py-10
        .col-11.col-sm-10.col-md-8.col-lg-6.col-xl-5.z-index-1
          .p-4.p-sm-5.bg-glass.border.border-2.border-dark.parallax.overflow-hidden.rounded(data-zanim-trigger="scroll" data-zanim-timeline!={delay: 0.6} data-rellax-speed=3)
            .overflow-hidden
              h4.fs-8.fs-sm-7
                span.d-block.overflow-hidden
                  span.d-inline-block(data-zanim-xs!={delay: 0.1}) your interior design
                span.d-block.overflow-hidden
                  span.d-inline-block(data-zanim-xs!={delay: 0.1}) expert, dedicated for
                span.d-block.overflow-hidden
                  span.d-inline-block(data-zanim-xs!={delay: 0.1}) creating beautiful home
            .overflow-hidden
              p.mt-4.mb-1.fw-bold.text-dark(data-zanim-xs!={delay: 0.3}) Address:
                br.d-block.d-md-none
                span.fw-normal 449, unknown street, Paris, France
            .overflow-hidden
              p.fw-bold.text-dark(data-zanim-xs!={delay: 0.4}) Contact:
                a(href="tel:+12345678").text-body.fs-9.text-serif.fw-normal +12345678, 
                a(href="tel:+87654321").text-body.fs-9.text-serif.fw-normal +87654321 
            .overflow-hidden
              div(data-zanim-xs!={delay: 0.6})
                a(href="#interior-projects").btn.btn-dark.btn-sm.mt-4 see works

